{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "project-tracker-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/projectTracker/ProjectTrackerWebPart.js",
          "manifest": "./src/webparts/projectTracker/ProjectTrackerWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js",
      "globalName": "jquery"
    },
    "jquery-ui": {
      "path": "https://code.jquery.com/ui/1.12.1/jquery-ui.js",
      "globalName": "jquery-ui"
    },
    "bootstrap": {
      "path": "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js",
      "globalName": "bootstrap",
      "globalDependencies": ["jquery"]
    }
  },
  "localizedResources": {
    "ProjectTrackerWebPartStrings": "lib/webparts/projectTracker/loc/{locale}.js"
  }
}
